{
  "name": "accella",
  "version": "1.2.0",
  "description": "A web framework built with Astro and Accel Record",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "accel": "dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./locales": {
      "types": "./dist/locales.d.ts",
      "default": "./dist/locales.js"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "default": "./dist/session.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./initialize": {
      "types": "./dist/initialize.d.ts",
      "default": "./dist/initialize.js"
    },
    "./integration": {
      "types": "./dist/integration.d.ts",
      "default": "./dist/integration.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koyopro/accella.git"
  },
  "keywords": [
    "accella",
    "accel-record",
    "astro"
  ],
  "files": [
    "dist",
    "bin"
  ],
  "author": "koyopro <koyopro2.0@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/koyopro/accella/issues"
  },
  "homepage": "https://github.com/koyopro/accella#readme",
  "dependencies": {
    "@mojojs/path": "^1.7.0",
    "accel-web": "^1.1.4",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^12.0.0"
  }
}
